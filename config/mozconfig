if [ "${BUILD_TYPE}" = "artifact" ]; then
  ac_add_options --enable-artifact-builds
  if [ "${BUILD_LEVEL}" = "dbg" ]; then
    ac_add_options --enable-debug
  fi
else
  if [ "${BUILD_LEVEL}" = "dbg" ]; then
    ac_add_options --enable-debug
    ac_add_options --disable-optimize
  else
    ac_add_options --enable-optimize
  fi
fi
