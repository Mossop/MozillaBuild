if [ -f $BUILD_BASE/config/.config ]; then
	. $BUILD_BASE/config/.config
fi

if [ -f $BUILD_BASE/config/$BUILD_CONFIG.config ]; then
	. $BUILD_BASE/config/$BUILD_CONFIG.config
fi

if [ ! "$MOZ_OBJDIR" ]; then
	MOZ_OBJDIR=$BUILD_BASE/build/$BUILD_CONFIG
fi

export MOZ_OBJDIR
export BUILD_WATCH
export BUILD_APP
export BUILD_CONFIG
